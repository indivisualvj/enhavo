<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="Enhavo\Bundle\ContactBundle\Entity\Contact" table="contact_contact"
            repository-class="Enhavo\Bundle\ContactBundle\Repository\ContactRepository">
        <id name="id" type="integer" column="id">
            <generator />
        </id>
        <field name="email" length="255" nullable="true" />
        <field name="token" length="255" nullable="true" />
        <field name="createdAt" type="datetime" nullable="true" />
    </entity>
</doctrine-mapping>
